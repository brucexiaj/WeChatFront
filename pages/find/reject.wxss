.inner.contentBox{background-color:#fff; width:100%; z-index:5; padding:10px; transition:left 0.2s ease-in-out; height:110px; border-bottom:1px solid #ddd; border-top:1px solid #ddd;} 
.inner.BtnBox{height:100%;width: 70px;text-align: center; z-index: 4; right: 0;text-align:center;} 
.inner.BtnBox image{display:block;width:45px;margin:auto;margin-top:33px;}
.mainPic{float:left;margin:9px;width:97px;height:97px;border-radius: 10px;overflow:hidden;}
.contentBox .info{float:left;width:calc(100% - 160px);position:relative;margin-top:10px;}
.contentBox .info .name{font-size:14px;color:#4C4C4C;letter-spacing:0.19px;line-height:18px;text-align:left;margin-bottom:3px;-webkit-box-orient: vertical; -webkit-line-clamp: 2; overflow: hidden;text-overflow:ellipsis;display:-webkit-box;}
.contentBox .info .brand{font-size:14px;color:#4C4C4C;letter-spacing:0.19px;line-height:18px;text-align:left;margin-bottom:3px;-webkit-box-orient: vertical; -webkit-line-clamp: 2; overflow: hidden;text-overflow:ellipsis;display:-webkit-box;}
.contentBox .info .priceRange{color:#FF2E38;font-size:16px;margin-top:5px;margin-bottom:5px;}
.contentBox .info .address{font-size: 13px; color: #B7B7B7;line-height:18px;width:100%;text-align:left;margin-bottom:1px;-webkit-box-orient: vertical; -webkit-line-clamp: 2; overflow: hidden;text-overflow:ellipsis;display:-webkit-box;}
.info .price{float:left;font-size: 13px; color: #4A4A4A;margin-right:5px;height:25px;line-height:25px;text-decoration:line-through }
.info .time{opacity: 0.7; font-size: 12px; color: #4A4A4A;float:left;height:25px;line-height:28px;float:right}
.info .timeIcon{float:left;margin-right:3px;width:12px;margin-top:8px;float:right}
.info .detail{margin-top:2px}
.placeholder{font-size: 14px; color: rgba(74,74,74,.2);}
.agreeConfirm{margin:auto; text-align:center; font-size:14px; color:#FFFFFF; width:133px; height:42px; line-height:42px; background-image:linear-gradient(-129deg, #E5C297 0%, #A97D43 100%); box-shadow:0 4px 10px 0 rgba(198,171,146,0.52); border-radius:20px; margin-top:12px;}
.tipPurchase{font-size:14px; color:#4A4A4A; height:34px; line-height:34px; text-align:center; border-top:3px solid #f2f2f2;}
.skuList{margin:0px 20px 7px 20px; border-bottom:1px solid #f2f2f2;}
.skuInfo{width:100%;overflow:hidden;display:-webkit-box;height:40px;line-height:40px;}
.skuInfo .name{font-size:14px;color:#4A4A4A;width:50%;text-align:left;height:40px;line-height:40px;}
.skuInfo .price{width:25%;font-size:12px;line-height:40px;height:40px;overflow:hidden;text-align:left;color:#9b9b9b}
.skuInfo .price text{font-size: 13px;color: #FF2E38;margin-right:10px;}
.skuInfo .price .costPrice{color:#4A4A4A;text-decoration: line-through}
.amount,.limit{display:inline-block;background: #FFFFFF;margin-right: 10px; border: 1px solid #979797; border-radius: 10px;width: 80px;height: 34px;text-align: center;font-size: 14px;}
.limitBox{display:inline-block;line-height:34px;font-size: 14px; color: #4A4A4A;margin-right: 8px;padding-left: 10px;width: 90px;height: 34px;background: #FFFFFF; border: 1px solid #979797; border-radius: 10px;}
.limitBox input{width:67px;float:right;display:inline-block;height:35px;line-height:35px;}
.fixedBox{height:67px; position:fixed; bottom:0px; left:0px; z-index:11; width:100%; border-top:2px solid #f2f2f2;}
.back{font-size: 14px; color: #4A4A4A;text-align:center;position: absolute;left: 35px;bottom: 13px;background: #FFFFFF;box-shadow: 0 4px 10px 0 rgba(198,171,146,0.28);border-radius: 20px;width: 35%;height: 40px;border: 1px solid #C6AB92;line-height: 40px}
.confirm{font-size: 14px; color: #fff;text-align:center;position: absolute;right: 35px;bottom: 13px;width:35%;height: 42px;background-image: linear-gradient(-129deg, #E5C297 0%, #A97D43 100%); box-shadow: 0 4px 10px 0 rgba(198,171,146,0.52); border-radius: 20px;line-height: 42px}
.reason{padding:15px;font-size:14px;color:#4a4a4a;}